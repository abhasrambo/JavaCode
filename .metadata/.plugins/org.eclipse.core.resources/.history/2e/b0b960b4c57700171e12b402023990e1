package parenthesisMatch;
import java.util.*;
public class parmatcher {
	public int mismatch(String s)
	{
	int count = 0, change = 0;
	char[] arr = s.toCharArray();
	Stack<Character> stack = new Stack<Character>();
	for(char all: arr)
	{
		switch (all)
		{
		case '(':
			count++;
			stack.push(all);
		case ')':
			stack.push(all);
			if(count==0)
			{
				stack.push('(');
				change++;
			}
			else
			{
				count--;
			}
		
		}
	}
	while(count-- >0)
	{
		stack.push(')');
		change++;
	}
	return change;
	}
	public static void main(String[] args)
	{
		String s = "(()))";
		parmatcher obj = new parmatcher();
		
	}
}
