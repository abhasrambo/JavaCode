package twoArrMerge;

import java.util.Scanner;

public class merge {
public static void main(String[] args)
{
	int[] a = new int[10];
	int[] b = new int[4];
	Scanner sc = new Scanner(System.in);
	for(int i = 0; i<6; i++)
	{
		a[i] = sc.nextInt();
	}
	for(int i = 0; i<4; i++)
	{
		b[i] = sc.nextInt();
	}
	
	for(int i =6; i<10;i++)
	{
		int j = 0;
		a[i]=b[j];
		j++;
	}
	for(int i = 0; i<a.length;i++)
	System.out.print(a[i]);
}
}
