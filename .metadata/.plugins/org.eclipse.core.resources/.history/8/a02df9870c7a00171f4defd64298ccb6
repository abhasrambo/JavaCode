package twoArrMerge;

import java.util.Scanner;

public class merge {
public static void main(String[] args)
{
	int[] a = new int[10];
	int[] b = new int[4];
	Scanner sc = new Scanner(System.in);
	for(int i = 0; i<(a.length-b.length); i++)
	{
		a[i] = sc.nextInt();
	}
	for(int i = 0; i<(b.length); i++)
	{
		b[i] = sc.nextInt();
	}
}
}
