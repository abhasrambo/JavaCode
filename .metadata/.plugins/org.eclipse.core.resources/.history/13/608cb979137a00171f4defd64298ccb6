package twoArrMerge;

import java.util.Scanner;

public class merge {
public static void main(String[] args)
{
	int[] a = new int[10];
	int[] b = new int[4];
	Scanner sc = new Scanner(System.in);
	for(int i = 0; i<6; i++)
	{
		a[i] = sc.nextInt();
	}
	for(int i = 0; i<4; i++)
	{
		b[i] = sc.nextInt();
	}
	int j = 0;
	for(int i =6; i<10;i++)
	{
		
		a[i]=b[j];
		j++;
	}
	int[] c = new int[10];
	for(int i =6; i<10;i++)
	{
		
		c[i] = a[i];
	}
	
	
	for( int i= 0;i<10;i++)
	{
		for( int k= 0;j<10;j++)
		{
			if( c[i]<c[k])
				{
				int temp;
				temp = c[i];
				c[i] = c[k];
				c[k] = temp;
				
				}
		}
	}
	for(int i = 0; i<c.length;i++)
	{
		System.out.print(c[i]);
	}
}
}
